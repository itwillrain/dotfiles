[ghq]
	root = /Users/takeshi_matsushita/gitserver
[user]
	name = Matsushita Takeshi
	email = matsushita@neopa.jp
[alias]
	s   = status -sb
	d   = diff
	a   = add
	aa  = add -A
	b   = branch
	co  = checkout
	cob = checkout -b
	c   = commit
	cm  = commit -m
	pso = push origin
	pl  = pullls
	plo = pull origin
	f   = fetch
	fup = fetch upstream
[core]
	excludesfile = /Users/takeshi_matsushita/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/takeshi_matsushita/.commit_template
[merge]
	ff = false
[pull]
	ff = only
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
