[ghq]
    root = $HOME/gitserver
[user]
	name = matsushita takeshi
    email = matsushitatakeshi@team-lab.com
[alias]
    s = status -sb
    d = diff
    a = add
    aa = add -A
    b = branch
    co = checkout
    cob = checkout -b
    c = commit
    cm = commit -m
    pso = push origin
    pl = pullls
    plo = pull origin
    f = fetch
    fup = fetch upstream
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
    template = $HOME/.commit_template
[pull]
	rebase = false
[init]
	defaultBranch = main
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
